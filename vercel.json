{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "tscOptions": {
          "sourceMap": false
        },
        "generateStaticRoutes": true,
        "serverFiles": [
          "package.json",
          "package-lock.json",
          "nuxt.config.ts"
        ]
      }
    }
  ]
}